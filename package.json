{
  "packageManager": "yarn@4.9.1",
  "name": "svgo-dev",
  "version": "0.0.0",
  "description": "Landing page and documentation for SVGO.",
  "license": "MIT",
  "homepage": "https://svgo.dev/",
  "private": true,
  "author": {
    "name": "Seth Falco",
    "email": "seth@falco.fun",
    "url": "https://falco.fun/"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/svgo"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/svg/svgo.dev.git"
  },
  "bugs": {
    "url": "https://github.com/svg/svgo.dev/issues"
  },
  "keywords": [
    "svgo",
    "docusaurus"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "svgo:clone": "rimraf .svgo && git clone https://github.com/svg/svgo.git .svgo --depth 1",
    "svgo:copy": "rimraf .svgo && cp -r ../svgo .svgo",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:types": "tsc",
    "qa": "yarn run lint && yarn run lint:types",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-client-redirects": "3.7.0",
    "@docusaurus/plugin-content-docs": "3.7.0",
    "@docusaurus/plugin-content-pages": "3.7.0",
    "@docusaurus/plugin-sitemap": "3.7.0",
    "@docusaurus/plugin-svgr": "3.7.0",
    "@docusaurus/theme-classic": "3.7.0",
    "@docusaurus/theme-common": "3.7.0",
    "@docusaurus/theme-live-codeblock": "3.7.0",
    "@easyops-cn/docusaurus-search-local": "^0.40.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-stringify": "^10.0.0",
    "svgo": "^4.0.0-rc.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "@eslint/js": "^9.27.0",
    "@types/react": "^18.3.1",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
